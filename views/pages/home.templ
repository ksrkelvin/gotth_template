package pages

import "encontradev/views/components"

templ HomeContent() {
    <div>
        <h1 class="text-3xl md:text-4xl font-bold mb-4 text-gray-800">Bem-vindo!</h1>
        <p class="text-gray-700 text-lg md:text-xl leading-relaxed">
            Conteúdo da página aqui. Você pode adicionar seções, imagens ou cards conforme precisar.
        </p>
    </div>
}

templ Home(title string) {
	@components.Layout(title, HomeContent())  
}
